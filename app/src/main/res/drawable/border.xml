<?xml version="1.0" encoding="UTF-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true" >
        <shape>
            <solid
                android:color="#99EAEAEA" />
            <stroke
                android:width="1dp"
                android:color="#A6A6A6" />
            <corners
                android:radius="13dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient
                android:startColor="#1af3f3f3"
                android:endColor="#1af3f3f3"
                android:angle="270" />
            <stroke
                android:width="1dp"
                android:color="#A6A6A6" />
            <corners
                android:radius="13dp" />
            <padding
                android:left="0dp"
                android:top="0dp"
                android:right="0dp"
                android:bottom="0dp" />
        </shape>
    </item>
</selector>